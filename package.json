{
  "name": "@huangjs888/transition",
  "version": "1.1.1",
  "description": "style transition and animation",
  "main": "lib/index.js",
  "types": "es/index.d.ts",
  "module": "es/index.js",
  "unpkg": "dist/transition.js",
  "exports": {
    "umd": "./dist/transition.min.js",
    "types": "./es/index.d.ts",
    "import": "./es/index.js",
    "default": "./lib/index.js"
  },
  "scripts": {
    "start": "npm run build",
    "build": "npm run clean && npm run lint && npm run build:types && npm run build:esm && npm run build:cjs && npm run build:umd",
    "build:merge": "npm run clean && npm run lint && npm run build:types:merge && npm run build:esm:merge && npm run build:cjs:merge && npm run build:umd",
    "build:cjs": "cross-env MOD_ENV=cjs BABEL_ENV=babel babel --extensions .ts src -d lib",
    "build:esm": "cross-env MOD_ENV=esm BABEL_ENV=babel babel --extensions .ts src -d es",
    "build:types": "tsc --declaration",
    "build:cjs:merge": "cross-env NODE_ENV=development MOD_ENV=cjs rollup -c --bundleConfigAsCjs",
    "build:esm:merge": "cross-env NODE_ENV=development MOD_ENV=esm rollup -c --bundleConfigAsCjs",
    "build:types:merge": "tsc --outFile es/transition.d.ts --declaration",
    "build:umd": "npm run build:umd:dev && npm run build:umd:prod",
    "build:umd:dev": "cross-env NODE_ENV=development MOD_ENV=umd rollup -c --bundleConfigAsCjs",
    "build:umd:prod": "cross-env NODE_ENV=production MOD_ENV=umd rollup -c --bundleConfigAsCjs",
    "clean": "rimraf lib && rimraf dist && rimraf es",
    "lint": "eslint . --ext .js,.ts"
  },
  "files": [
    "es",
    "dist",
    "lib",
    "README.md"
  ],
  "keywords": [
    "huangjs",
    "huangjs888",
    "transition",
    "animation",
    "easing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huangjs888/transition.git"
  },
  "bugs": {
    "url": "https://github.com/huangjs888/transition/issues"
  },
  "homepage": "https://github.com/huangjs888/transition#readme",
  "author": {
    "name": "Huangjs",
    "email": "1170384651@qq.com"
  },
  "license": "MIT",
  "peerDependencies": {},
  "peerDependenciesMeta": {},
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.22.10"
  }
}
